; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Paradise Volley"
run/main_scene="res://scenes/intro.tscn"
boot_splash/image="res://assets/splash.png"
boot_splash/bg_color=Color( 0.509804, 0.976471, 1, 1 )
config/icon="res://assets/icon.png"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=800
window/size/height=480
window/handheld/emulate_touchscreen=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[rendering]

quality/filters/use_nearest_mipmap_filter=true
quality/shadow_atlas/quadrant_0_subdiv=2
quality/shadow_atlas/quadrant_1_subdiv=3
quality/shadows/filter_mode=2
quality/reflections/high_quality_ggx=false
quality/shading/force_vertex_shading=true
quality/subsurface_scattering/quality=2
quality/voxel_cone_tracing/high_quality=false
environment/default_clear_color=Color( 0.511719, 0.977112, 1, 1 )
quality/reflections/atlas_size=1024
quality/reflections/atlas_subdiv=4
quality/filters/msaa=3
environment/default_environment="res://default_env.tres"
